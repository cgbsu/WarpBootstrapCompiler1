let my_other_function(my_parameter) { (0u32 + my_parameter) + factorial(my_parameter * 2) };
let my_function(my_parameter) { (0u32 + factorial(my_parameter * 2)) + (0u32 + my_parameter) };
let factorial(to_factor : {(0u32 + to_factor) <= 1u32}) { 1u32 };
let factorial(to_factor : {(0u32 + to_factor) > 1u32}) { (0u32 + to_factor) * factorial(to_factor - 1u32) };

